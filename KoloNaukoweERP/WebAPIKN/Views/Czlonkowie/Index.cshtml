@model IEnumerable<DAL.Entities.Czlonek>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PelnionaFunkcja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NrTelefonu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nazwisko)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Imie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KierunekStudiow)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Wydzial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Uczelnia)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PelnionaFunkcja.Nazwa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NrTelefonu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nazwisko)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Imie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KierunekStudiow)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Wydzial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Uczelnia)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCzlonka">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCzlonka">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCzlonka">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
